@Article{Zaghloul:2017:ASE,
  author =       "Mofreh R. Zaghloul",
  title =        "Algorithm 985: Simple, Efficient, and Relatively
                 Accurate Approximation for the Evaluation of the
                 {Faddeyeva} Function",
  journal =      j-TOMS,
  volume =       "44",
  number =       "2",
