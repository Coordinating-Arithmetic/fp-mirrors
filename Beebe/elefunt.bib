  ISSN-L =       "0006-3835",
  bibdate =      "Wed Jan 4 15:06:04 MST 2006",
  bibsource =    "http://springerlink.metapress.com/openurl.asp?genre=issue&issn=0006-3835&volume=41&issue=3;
